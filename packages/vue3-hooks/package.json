{
  "name": "vue3-hooks",
  "version": "1.0.0",
  "description": "vue3-app",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint --fix",
    "lint": "npx lint-staged",
    "stylelint": "npx stylelint --fix"
  },
  "dependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.1",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-istanbul": "0.34.2",
    "@vitest/ui": "0.34.2",
    "@vue/test-utils": "^2.4.1",
    "eslint": "8.47.0",
    "happy-dom": "10.10.4",
    "less": "4.2.0",
    "lint-staged": "^14.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitest": "^0.32",
    "vue-tsc": "1.0.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": [
      "npm run eslint"
    ],
    "src/**/*.{css,less,vue}": [
      "npm run stylelint"
    ]
  }
}
